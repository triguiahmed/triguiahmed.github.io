      .loader {
        border: 6px solid #f3f3f3;
        /* Light grey */
        border-top: 6px solid #4482da;
        /* Blue */
        border-radius: 50%;
        width: 60px;
        height: 60px;
        animation: spin 2s linear infinite;
        position: fixed;
        top: 45%;
        left: 50%;
        transform: translate(-50%, -50%);
      }

      @keyframes spin {
        0% {
          transform: rotate(0deg);
        }

        100% {
          transform: rotate(360deg);
        }
      }

      .progress-bar {
        background: #5593da;
        border-color: #5593da;
      }

      .btn-primary {
        background: #5593da;
        border-color: #5593da;
      }

      .btn-primary:hover {
        background: #4482da;
        border-color: #4482da;
      }

      .portfolio-item {
        width: 100%;
        height: 100%;
      }

      .grid-item {
        margin-left: 4%;
        margin-right: 4%;
      }

      .youtube-player.pristine::before {
        content: '';
        position: absolute;
        top: 50%;
        left: 50%;
        margin-top: -25px;
        margin-left: -25px;
        height: 0;
        width: 0;
        border-left: 50px solid #5593da;
        border-top: 30px solid transparent;
        border-bottom: 30px solid transparent;
        transition: opacity .2s ease;
      }

      .youtube-player.pristine:hover::before {
        transition-delay: 0.2s;
        opacity: .7;
      }
    </style>
