<script>
  const div = document.getElementById('extraControls'); // Get element from DOM
  const div1 = document.getElementById('loader'); // Get element from DOM
  Promise.all(Array.from(document.images).filter(img => !img.complete).map(img => new Promise(resolve => {
    img.onload = img.onerror = resolve;
  }))).then(() => {
    div.classList.remove('hidden');
    div1.classList.remove('loader');
  });
</script>
<script defer>
  var players = document.querySelectorAll('.youtube-player')
  var loadPlayer = function(event) {
    this.classList.remove("mb-3");
    this.classList.add("mb-0");
    p = this.parentElement;
    caption = p.querySelector('figcaption');
    caption.remove();
    var target = event.currentTarget
    var iframe = document.createElement('iframe')
    iframe.height = target.clientHeight
    iframe.width = target.clientWidth
    iframe.src = 'https://www.youtube.com/embed/' + target.dataset.videoId + '?autoplay=1&vq=hd1080&modestbranding=1&autohide=1&showinfo=0&controls=0'
    iframe.setAttribute('frameborder', 0)
    iframe.setAttribute('allowfullscreen', 0)
    iframe.setAttribute('allow', "accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture")
    iframe.setAttribute('style', "border-radius: 8px; overflow: hidden;")
    target.classList.remove('pristine')
    if (target.children.length) {
      target.replaceChild(iframe, target.firstElementChild)
    } else {
      target.appendChild(iframe)
    }
  }
  var config = {
    once: true
  }
  Array.from(players).forEach(function(player) {
    player.addEventListener('click', loadPlayer, config)
  })
</script>
